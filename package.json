{
	"name": "simple-presence",
	"private": true,
	"packageManager": "bun@1.2.17",
	"type": "module",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"check": "biome check --write --unsafe .",
		"build": "bun run --filter '*' build",
		"dev": "bun run --filter '*' dev",
		"deploy": "bun run --filter '*' deploy"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.5"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@latest"
	},
	"patchedDependencies": {
		"hono@4.8.3": "patches/hono@4.8.3.patch"
	}
}
